     user_id: this.user_id,\r\n        },\r\n        body: JSON.stringify({\r\n          user_id: this.new_user_id,\r\n          verified: \"true\",\r\n        }),\r\n      })\r\n        .then((response) => response.json())\r\n        .then((data) => {\r\n          if (data.category == \"success\") {\r\n            this.flashMessage.success({\r\n              message: \"Successfully Verified\",\r\n            });\r\n            this.show_component = false;\r\n          }\r\n          if (data.category == \"error\") {\r\n            this.flashMessage.error({\r\n              message: data.message,\r\n            });\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          this.$log.error(`Error : ${error}`);\r\n          this.flashMessage.error({\r\n            message: \"Internal Server Error\",\r\n          });\r\n        });\r\n    },\r\n  },\r\n  computed: {},\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.user-card-container {\r\n  box-shadow: 2px 4px 5px 5px #dbdada;\r\n  /* background-color: rgb(251, 252, 252); */\r\n  margin: 12px 15px;\r\n  /* padding: 10px; */\r\n}\r\n/* .info-card-container:hover {\r\n  box-shadow: 5px 8px 8px 10px #888888;\r\n   background-color: rgb(255, 255, 255); \r\n} */\r\n</style>\r\n"],"mappings":"AAuBA,YAAAA,MAAA;AACA;EACAC,IAAA;EACAC,KAAA;EACAC,UAAA;EACAC,KAAA;IACA;MACAC,OAAA,OAAAC,MAAA,CAAAC,OAAA,CAAAC,WAAA;MACAC,WAAA,OAAAC,SAAA;MACAC,cAAA,OAAAC;IACA;EACA;EACAC,QAAA;EACAC,QAAA;EACAC,OAAA;IACAC,KAAA;MACA,KAAAP,WAAA,QAAAC,SAAA;IACA;IACAO,eAAA,iBAAAA,CAAAC,KAAA;MACAC,KAAA,CAAAnB,MAAA,CAAAoB,iBAAA,YAAAX,WAAA;QACAY,MAAA;QACAC,OAAA;UACA;UACAC,SAAA,OAAAjB,MAAA,CAAAC,OAAA,CAAAiB,aAAA;UACAnB,OAAA,OAAAA;QACA;QACAoB,IAAA,EAAAC,IAAA,CAAAC,SAAA;UACAtB,OAAA,OAAAI;QACA;MACA,GACAmB,IAAA,CAAAC,QAAA,IAAAA,QAAA,CAAAC,IAAA,IACAF,IAAA,CAAAxB,IAAA;QACA,IAAAA,IAAA,CAAA2B,QAAA;UACA,KAAAC,YAAA,CAAAC,OAAA;YACAC,OAAA;UACA;UACA,KAAAvB,cAAA;QACA;QACA,IAAAP,IAAA,CAAA2B,QAAA;UACA,KAAAC,YAAA,CAAAG,KAAA;YACAD,OAAA,EAAA9B,IAAA,CAAA8B;UACA;QACA;MACA